<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tian.tas.mapper.ScoreMapper">

    <resultMap id="BaseResultMap" type="org.tian.tas.entity.Score">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="courseNum" column="course_num" jdbcType="VARCHAR"/>
            <result property="studentNum" column="student_num" jdbcType="INTEGER"/>
            <result property="num" column="num" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,course_num,student_num,
        num
    </sql>
</mapper>
