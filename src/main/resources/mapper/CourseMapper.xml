<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tian.tas.mapper.CourseMapper">

    <select id="selectCNByTeacherNum" resultType="course">
        select * from tas_tmh.course
        <where>
            teacher_num = #{teacherNum}
        </where>
    </select>


</mapper>
